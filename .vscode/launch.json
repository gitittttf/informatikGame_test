{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Informatik Game",
            "request": "launch",
            "mainClass": "com.informatikgame.core.GuiMain",
            "projectName": "informatikgame",
            "vmArgs": "-Dlanterna.terminal-type=swing -Dfile.encoding=UTF-8",
            "console": "internalConsole",
            "preLaunchTask": "mvnw-compile",
            "classPaths": [
                "${workspaceFolder}/target/classes",
                "${userHome}/.m2/repository/com/googlecode/lanterna/lanterna/3.1.3/lanterna-3.1.3.jar"
            ]
        }
    ]
}